FUN oopify(prefix) -> prefix + "oop"

FUN juntar(elementos, separador)
	VAR result = ""
	VAR len = LEN(elementos)

	FOR i = 0 TO len THEN
		VAR result = result + elementos/i
		IF i != len - 1 THEN VAR result = result + separador
	END

	RETURN result
END

FUN mapear(elementos, func)
	VAR nuevo_elemento = []

	FOR i = 0 TO LEN(elementos) THEN
		APPEND(nuevo_elemento, func(elementos/i))
	END

	RETURN nuevo_elemento
END

